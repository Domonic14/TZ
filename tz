describe('Тестирование формы обратной связи', () => {
    it('Успешная отправка формы с корректными данными', () => {
        // Открыть страницу с формой
        cy.visit('https://www.testograf.ru/ru/blog/feedback-form-template');

        // Заполнить поля формы
        cy.get('input[name="name"]').type('Тест Имя'); // Замените на правильный селектор
        cy.get('input[name="email"]').type('test@example.com'); // Замените на правильный селектор
        cy.get('textarea[name="message"]').type('Это тестовое сообщение.'); // Замените на правильный селектор

        // Отправить форму
        cy.get('button[type="submit"]').click(); // Замените на правильный селектор

        // Проверить, что отображается сообщение об успешной отправке
        cy.get('.success-message') // Замените на правильный селектор для сообщения об успешной отправке
            .should('be.visible')
            .and('contain', 'Спасибо за ваше сообщение!'); // Замените на текст сообщения об успешной отправке
    });
});
